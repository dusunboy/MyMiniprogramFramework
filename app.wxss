/**app.wxss**/

/* margin padding 简写样式 */
@import './styles/dimen.wxss';

.container {
  height: 100%;
  padding-top: 61px;
}

.flex-row {
  display: flex;
  flex-direction: row;
}
.flex-row-reverse {
  display: flex;
  flex-direction: row-reverse;
}
.flex-column {
  display: flex;
  flex-direction: column;
}
.flex-column-reverse {
  display: flex;
  flex-direction: column-reverse;
}
