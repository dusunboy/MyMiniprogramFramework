<!--pages/demo/demo.wxml-->
<navigation-bar  show-back="{{isShowBack}}" bindback="onBack" enable="{{isNavEnable}}" show-loading="{{isNavLoading}}" title="{{navTitle}}"  title-style="{{navTitleStyle}}" bg-style="{{navBgStyle}}" text-style='{{navTextStyle}}'></navigation-bar>

<view class="container flex-column">
  <button bindtap="testRequest" class="ml100">测试请求</button>
  <button bindtap="testDownload" >测试下载</button>
  <my-components>
    <view slot="before">这里是插入到组件slot name="before"中的内容</view>
    <view slot="after">这里是插入到组件slot name="after"中的内容</view>
  </my-components>
</view>
