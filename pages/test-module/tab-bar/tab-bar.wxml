<!--pages/test-module/tab-bar/tab-bar.wxml-->
<c-navigation-bar loading="{{isNavLoading}}" show="{{isShowNav}}" animated="{{isNavAnimated}}" 
  color="{{navTitleColor}}" background="{{navBgStyle}}" title="{{navTitle}}" back="{{isShowNavBack}}" mode="{{navMode}}">
</c-navigation-bar>
<c-my-components selected="{{selected}}" tabBarHeight="{{tabBarHeight}}" >
  <view slot="before">这里是插入到组件slot name="before"中的内容</view>
  <view slot="after">这里是插入到组件slot name="after"中的内容</view>
</c-my-components>
<c-my-components1 selected="{{selected}}" tabBarHeight="{{tabBarHeight}}">
  <view slot="before">这里是插入到组件slot name="before"中的内容111111111111111</view>
  <view slot="after">这里是插入到组件slot name="after"中的内容1111111111111111</view>
</c-my-components1>
<tab-bar id="" selected="{{selected}}" color="{{color}}"
  selectedColor="{{selectedColor}}" list="{{list}}" bind:switchTab="switchTab">
</tab-bar>
